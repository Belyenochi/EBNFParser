Test[Lambdef['lambda(?!\S)'[lambda]
             
             NameList[Name[x]
                      
                      ','[,]
                      
                      Name[y]
                      
             ]
             ':'[:]
             
             Test[Lambdef['lambda(?!\S)'[lambda]
                          
                          NameList[Name[z]
                                   
                                   ','[,]
                                   
                                   Name[d]
                                   
                          ]
                          ':'[:]
                          
                          Test[OrTest[AndTest[NotTest[Arith[Term[Factor[AtomExpr[Atom[Name[x]
                                                                                      
                                                                                 ]
                                                                        ]
                                                                 ]
                                                            ]
                                                            '+'[+]
                                                            
                                                            Term[Factor[AtomExpr[Atom[Name[y]
                                                                                      
                                                                                 ]
                                                                        ]
                                                                 ]
                                                                 '*'[*]
                                                                 
                                                                 Factor[AtomExpr[Atom['('[(]
                                                                                      
                                                                                      ListComp[Test[OrTest[AndTest[NotTest[Arith[Term[Factor[AtomExpr[Atom[Number[1]
                                                                                                                                                           
                                                                                                                                                      ]
                                                                                                                                             ]
                                                                                                                                      ]
                                                                                                                                 ]
                                                                                                                                 '+'[+]
                                                                                                                                 
                                                                                                                                 Term[Factor[AtomExpr[Atom[Number[2]
                                                                                                                                                           
                                                                                                                                                      ]
                                                                                                                                             ]
                                                                                                                                      ]
                                                                                                                                 ]
                                                                                                                           ]
                                                                                                                   ]
                                                                                                           ]
                                                                                                    ]
                                                                                               ]
                                                                                      ]
                                                                                      ')'[)]
                                                                                      
                                                                                 ]
                                                                        ]
                                                                 ]
                                                            ]
                                                      ]
                                              ]
                                      ]
                               ]
                          ]
                  ]
